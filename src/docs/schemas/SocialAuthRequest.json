{
  "SocialAuthRequest": {
    "type": "object",
    "required": ["email", "firstName", "lastName", "sub"],
    "properties": {
      "email": {
        "type": "string",
        "format": "email"
      },
      "firstName": {
        "type": "string"
      },
      "lastName": {
        "type": "string"
      },
      "image": {
        "type": "string",
        "nullable": true
      },
      "sub": {
        "type": "string",
        "example": "google-oauth-sub"
      }
    }
  }
}
